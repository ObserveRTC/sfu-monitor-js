{
  "name": "sfu-observer-js",
  "version": "0.1.1",
  "description": "ObserveRTC client library to observe SFUs, make samples and send to a server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ObserveRTC/sfu-observer-js.git"
  },
  "keywords": [
    "sfu",
    "webrtc",
    "observer"
  ],
  "author": "Balazs Kreith",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ObserveRTC/sfu-observer-js/issues"
  },
  "homepage": "https://github.com/ObserveRTC/sfu-observer-js#readme",
  "devDependencies": {
    "@types/event-emitter": "^0.3.3",
    "@types/jest": "^26.0.24",
    "@types/ws": "^7.4.7",
    "jest": "^27.0.6",
    "jest-websocket-mock": "^2.2.1",
    "mock-socket": "^9.0.3",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typed-emitter": "^1.3.1",
    "typescript": "^4.4.2",
    "url-parse": ">=1.5.2"
  },
  "dependencies": {
    "@types/node": "^16.7.2",
    "@types/websocket": "^1.0.4",
    "typescript-logging": "^1.0.0",
    "uuidv4": "^6.2.12",
    "ws": "^7.5.3"
  }
}
