{
  "name": "@observertc/sfu-observer-js",
  "version": "1.0.0-beta.6",
  "description": "ObserveRTC SFU Integration Core Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src/*",
    "build": "tsc",
    "test": "jest --config jest.config.js --runInBand",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "webrtc",
    "getStats",
    "observertc",
    "sfu",
    "integration"
  ],
  "author": "Balazs Kreith",
  "license": "Apache-2.0",
  "dependencies": {
    "@observertc/schemas": "^2.0.0-beta.22",
    "@types/events": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.2.3",
    "bowser": "^2.11.0",
    "js-base64": "^3.7.2",
    "js-sha256": "^0.9.0",
    "loglevel": "^1.8.0",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "bufferutil": "^4.0.6",
    "eslint": "^8.9.0",
    "jest": "^27.5.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "utf-8-validate": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/observertc/sfu-observer-js"
  }
}
